{
  "name": "komicle",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon server/app.js --exec babel-node",
    "seed-add": "sequelize seed:create --name users",
    "seed": "sequelize db:seed:all",
    "generate-model": "sequelize model:create --name Like --attributes like:boolean,episodeId:integer,userId:integer",
    "migrate-undo": "sequelize db:migrate:undo:all",
    "migrate-db": "sequelize db:migrate",
    "migrate": "npm run migrate-undo && npm run migrate-db && npm run seed",
    "test": "export NODE_ENV=test || set NODE_ENV=test && nyc --reporter=html --reporter=text mocha server/tests/ --exit --require babel-core/register",
    "test-watch": "nodemon --exec \"npm test\"",
    "pretest": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codeniro/komicle.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Codeniro/komicle/issues"
  },
  "homepage": "https://github.com/Codeniro/komicle#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.8.7",
    "sequelize-cli": "^5.4.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "faker": "^4.1.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
